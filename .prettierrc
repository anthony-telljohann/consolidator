{
  "semi": false,
  "singleQuote": true,
  "overrides": [
    {
                              "files": "*.json",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": ".*rc",
      "options": {
        "parser": "json"
      }
    },
    {
                      "files": "*.{css,less,scss}",
                            "options": {
        "parser": "postcss"
      }
    }
  ]
}
